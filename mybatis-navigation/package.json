{
    "name": "mybatis-lite-navigation",
    "displayName": "MyBatis Lite Navigation",
    "description": "Lightweight, configuration-free bidirectional navigation for MyBatis.",
    "icon": "icon.png",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mybatis-lite.gotoMapper",
                "title": "Go to MyBatis Mapper"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "jest --coverage"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^16.x",
        "@types/vscode": "^1.80.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.0"
    }
}